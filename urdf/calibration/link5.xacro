<?xml version="1.0"?>

<robot name="calibration_skin" xmlns:xacro="http://ros.org/wiki/xacro">
  
  <!-- Include the link5 skin macro -->
  <xacro:include filename="$(find gentact_descriptions)/urdf/skin/link5_fancy.xacro" />

  <!-- Create a base link for the calibration skin -->
  <link name="calibration_base" />
  
  <!-- Instantiate the link5 skin -->
  <xacro:link5_skin 
    parent="calibration_base" 
    namespace="calibration" />
</robot>